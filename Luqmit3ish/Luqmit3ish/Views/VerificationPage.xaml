<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Luqmit3ish.Views.VerificationPage">
    <ContentPage.Content>
        <StackLayout Margin="16,64,16,35" VerticalOptions="Center">
            <Label Text="Verification"/>
            <Button VerticalOptions="Center" Text="Continue" Command="{Binding SignupCommand}" Style="{StaticResource BorderButton}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>