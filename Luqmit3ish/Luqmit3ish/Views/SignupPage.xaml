<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Luqmit3ish.Views.SignupPage">
    <ContentPage.Content>
        <StackLayout Margin="16,64,16,35"  VerticalOptions="Center">
            <Button VerticalOptions="Center" Text="Signup" Clicked="SignupClicked" Style="{StaticResource BorderButton}"/>
            <Button Text="Login" Clicked="LoginClicked" Style="{StaticResource NoBorderButton}"/>
            <Label Text="&#xf015;" FontFamily="FontAwesomeIcons" FontSize="16" />
        </StackLayout>

    </ContentPage.Content>



</ContentPage>